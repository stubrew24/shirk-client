{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "dateformat": "^3.0.3",
    "electron-is-dev": "^1.1.0",
    "electron-packager": "^14.0.4",
    "glamor": "^2.20.40",
    "react": "^16.9.0",
    "react-bell-chat": "^1.0.23",
    "react-dom": "^16.9.0",
    "react-infinite-scroll-reverse": "^1.0.1",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.0",
    "react-scroll": "^1.7.14",
    "react-scroll-to-bottom": "^1.3.2",
    "react-semantic-ui": "^0.2.0",
    "redux": "^4.0.4",
    "semantic-ui-react": "^0.88.0",
    "socket.io-client": "^2.2.0"
  },
  "main": "public/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"CryptoApp\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "electron": "^4.2.9",
    "electron-builder": "^20.38.5",
    "electron-devtools-installer": "^2.2.4",
    "prettier": "^1.18.2",
    "wait-on": "^3.2.0"
  }
}
